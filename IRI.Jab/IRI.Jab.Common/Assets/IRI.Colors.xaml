<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="mapTehranIrDarkRedColor">#FF900000</Color>
    <Color x:Key="mapTehranIrLightRedColor">#FFC75050</Color>
    <Color x:Key="mapTehranIrNormalRedColor">#FFAD0000</Color>
    
    <SolidColorBrush x:Key="mapTehranIrDarkRedBrush" Color="{StaticResource mapTehranIrDarkRedColor}"/>
    <SolidColorBrush x:Key="mapTehranIrLightRedBrush" Color="{StaticResource mapTehranIrLightRedColor}"/>
    <SolidColorBrush x:Key="mapTehranIrNormalRedBrush" Color="{StaticResource mapTehranIrNormalRedColor}"/>
</ResourceDictionary>
